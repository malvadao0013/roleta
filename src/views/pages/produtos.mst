    <script>
      // Definindo o tempo inicial em segundos
      let tempoRestante = 10 * 60;
      
      const minutosElemento = document.getElementById('minutos');
      const segundosElemento = document.getElementById('segundos');
      const mensagemElemento = document.getElementById('mensagem');
      
      const intervalo = setInterval(() => {
          const minutos = Math.floor(tempoRestante / 60);
          const segundos = tempoRestante % 60;
      
          minutosElemento.textContent = String(minutos).padStart(2, '0');
          segundosElemento.textContent = String(segundos).padStart(2, '0');
      
          if (tempoRestante <= 0) {
              clearInterval(intervalo);       
              mensagemElemento.textContent = "Última chance para escolher seu produto com 95% de desconto, não perca!";
          } else {
              tempoRestante--;
          }
      }, 1000);
      
    </script>
