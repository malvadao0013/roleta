    <script>
      setTimeout(function () {
          var button = document.querySelector("#recompensas");
          
          // Alterando a variável link para a nova URL
          var link = "/roleta";
          
          button.addEventListener("click", function () {
              window.location.href = link + window.location.search;
          });
      }, 1000);
      
      function validateQuestion(questionNumber) {
          var radios = document.getElementsByName("difficulty" + questionNumber);
          var isChecked = false;
          for (var i = 0; i < radios.length; i++) {
              if (radios[i].checked) {
                  isChecked = true;
                  break;
              }
          }
          if (!isChecked) {
              document.querySelector("#p" + questionNumber + " .error-message").style.display = "block";
          }
          return isChecked;
      }
      
      function showAd2() {
          if (validateQuestion(1)) {
              document.getElementById("p1").style.display = "none";
              document.getElementById("p2").style.display = "block";
              document.getElementById("progress1").style.display = "none";
              document.getElementById("progress2").style.display = "block";
          }
      }
      
